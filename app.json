{
  "expo": {
    "name": "Global-Times",
    "slug": "Global-Times",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/globalTimes.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#808080"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.globalnews.App",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/globalTimes.png"
    }
  }
}
